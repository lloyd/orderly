{
  "$schema": "http://json-schema.org/schema",
  "type": "object",
  "properties": {
    "mystring": {
      "type": "string",
      "minLength": 3,
      "maxLength": 12
    },
    "myint": {
      "type": "number",
      "minimum": 0,
      "maximum": 42
    },
    "mynum": {
      "type": "number",
      "minimum": 1.1,
      "maximum": 42.1
    },
    "myarray": {
      "type": "array",
      "items": {
        "type": "any"
      },
      "minItems": 0,
      "maxItems": 500
    }
  }
}
